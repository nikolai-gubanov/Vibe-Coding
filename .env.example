# Cisco FMC Connection Settings
FMC_HOST=192.168.1.100
FMC_USERNAME=api_automation
FMC_PASSWORD=YourSecurePassword123!

# SSL Verification (set to 'false' only in lab environments)
FMC_VERIFY_SSL=true

# Optional: Path to custom CA certificate
# FMC_CA_CERT=/path/to/ca-bundle.crt

# Logging Level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# API Rate Limiting
MAX_REQUESTS_PER_MINUTE=100

# Timeout Settings (in seconds)
API_TIMEOUT=30
